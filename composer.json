{
  "name": "femundfilou/kirby-vite",
  "description": "Use Vite Frontend Tooling in Kirby.",
  "version": "0.0.0",
  "type": "kirby-plugin",
  "keywords": ["kirby-plugin"],
  "license": "MIT",
  "homepage": "https://github.com/femundfilou/kirby-vite",
  "autoload": {
    "psr-4": {
      "Fefi\\Vite\\": "lib/"
    }
  },
  "authors": [
    {
      "name": "Justus Kraft",
      "email": "hallo@justuskraft.de"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "require": {
    "php": ">=7.1.0",
    "getkirby/cms": "^3.6",
    "getkirby/composer-installer": "^1.1"
  },
  "scripts": {
    "fix": "php-cs-fixer fix --config .php-cs-fixer.dist.php"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "installer-name": "vite"
}
